{
  "name": "filecat",
  "version": "1.0.3",
  "description": "filecat 文件管理器",
  "author": "xiaobaidadada",
  "scripts": {
    "dev": "node  --require ts-node/register ./shell/dev.js --env ./env",
    "dev-debug": "node --inspect=0.0.0.0:9001 --require ts-node/register ./shell/dev.js --env ./env",
    "server-dev": "node --require ts-node/register ./src/main/server.ts --env ./env",
    "webpack-dev": "npx webpack serve --mode development --config shell/config/webpack.config.js",
    "generate-proto": "node --require ts-node/register ./src/common/proto/proto.generate.ts",
    "hot-server-dev": "node --require ts-node/register ./src/main/piping.main.ts ",
    "webpack-build": "npx webpack  --config shell/config/webpack.config.js",
    "build": "tsc && node shell/build.js",
    "start": "node build/main.js",
    "pkg-win-amd64": "npm run build && pkg . --targets node18-win-x64 --output filecat-win.exe",
    "pkg-linux-amd64": "npm run build && pkg . --targets node18-linux-x64 --output filecat-linux"
  },
  "pkg": {
    "assets": [
      "build/dist/**/**",
      "node_modules/@xiaobaidadada/node-tuntap2-wintun/wintun_dll/*.dll"
    ]
  },
  "bin": {
    "filecat": "shell/filecat"
  },
  "keywords": [
    "administrator",
    "filemanager",
    "cpu",
    "docker",
    "linux",
    "文件服务器",
    "运维面板",
    "react",
    "ssh",
    "ftp",
    "systeminfo",
    "vpn",
    "docker",
    "ddns"
  ],
  "files": [
    "build/**/*",
    "shell",
    "env",
    "LICENSE.txt",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@homebridge/node-pty-prebuilt-multiarch": "^0.11.14",
    "@koa/cors": "^5.0.0",
    "@xiaobaidadada/node-tuntap2-wintun": "^1.0.6",
    "archiver": "^7.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "jmespath": "^0.16.0",
    "koa": "^2.15.2",
    "koa-body": "^6.0.1",
    "koa-proxies": "^0.12.4",
    "koa-router": "^12.0.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "needle": "^3.3.1",
    "node-forge": "^1.3.1",
    "node-process-watcher": "^1.0.2",
    "node-unrar-js": "^2.0.2",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "routing-controllers": "^0.10.4",
    "serialize-javascript": "^6.0.2",
    "socket.io-parser": "^4.2.4",
    "ssh2": "^1.15.0",
    "sshpk": "^1.18.0",
    "systeminformation": "^5.22.11",
    "tar": "^7.4.3",
    "tencentcloud-sdk-nodejs": "^4.0.858",
    "tldts": "^6.1.20",
    "typedi": "^0.10.0",
    "unzipper": "^0.12.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@excalidraw/excalidraw": "^0.17.6",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ace-builds": "^1.32.9",
    "axios": "^1.6.8",
    "babel-loader": "^9.1.3",
    "chart.js": "^4.4.2",
    "css-loader": "^6.10.0",
    "flv.js": "^1.6.2",
    "github-markdown-css": "^5.6.1",
    "html-webpack-plugin": "^5.6.0",
    "i18next": "^23.12.2",
    "listr2": "^8.2.1",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "material-icons": "^1.13.12",
    "normalize.css": "^8.0.1",
    "noty": "^3.2.0-beta-deprecated",
    "piping": "^1.0.0-rc.4",
    "pkg": "^5.8.1",
    "protobufjs-cli": "^1.1.3",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-router-dom": "^6.26.2",
    "recoil": "^0.7.7",
    "style-loader": "^3.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "video.js": "^8.17.2",
    "videojs-hotkeys": "^0.2.28",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  }
}
